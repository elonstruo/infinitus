#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_swagger-jsdoc@5.0.1@swagger-jsdoc/bin/swagger-jsdoc.js" "$@"
  ret=$?
else 
  node  "$basedir/../_swagger-jsdoc@5.0.1@swagger-jsdoc/bin/swagger-jsdoc.js" "$@"
  ret=$?
fi
exit $ret
